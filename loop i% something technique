// constructive string problem form codeforces ;

#include <bits/stdc++.h>

using namespace std;

int main() {

	
	int t;
	cin >> t;
	while (t--) {
		int n, a, b;
		cin >> n >> a >> b;
		for (int i = 0; i < n; ++i) {
			cout << char('a' + i % b);            // osthir technique; 
			                                      // 'a'+0 = a; 'a'+1 = b; 'a'+2 = c;
		}
		cout << endl;
	}
	
	return 0;
}
